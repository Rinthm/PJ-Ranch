<!DOCTYPE html>
<html>
<head>
  <title>PjRanch</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => false %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => false %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div id="box-main">
		<nav id="nav-main-wrap">
			<%= link_to image_tag("pj-logo.png", class: "pj-logo", alt: "PJ Ranch", title: "PJ logo"), '/' %>
			<ul class="nav-list">
				<li class="nav-item">
					ABOUT
				</li>
				<li class="nav-item">
					LOCAL
				</li>
				<li class="nav-item">
					BLOG
				</li>
				<li class="nav-item">
					CONTACT
				</li>
			</ul>
		</nav>

	<%= yield %>

	</div>
</body>
</html>
